{
  "name": "fist/facade",
  "description": "The Fistlab PHP Facade Components",
  "keywords": ["fistlab", "fistphp", "component", "facade"],
  "license": "MIT",
  "homepage": "https://github.com/fistphp/facade",
  "support": {
    "issues": "https://github.com/fistlab/php/issues",
    "source": "https://github.com/fistlab/php"
  },
  "authors": [
    {
      "name": "Mark Topper",
      "email": "mark@ulties.com"
    }
  ],
  "require": {
    "php": "~7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.0",
    "fist/testing": "self.version",
    "fist/container": "self.version"
  },
  "autoload": {
    "psr-4": {
      "Fist\\Facade\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "suggest": {
    "fist/container": "Required for using ContainerFacade"
  },
  "minimum-stability": "dev"
}
